/* Top of page ribbon */
/* ---------------------------------------------------------- */

#ribbon .ribbon-link {
  font-family: 'Quattrocento Sans', sans-serif;
  text-align: center; 
  padding-top: 8px;
}

#ribbon .ribbon-link a {
	color: #dedede;
	font-size: 16px;
	z-index: 9;
}

#ribbon {
	width: 300px;
	height: 90px;
	margin: 0 auto 0;
	position: relative;
	overflow: hidden;
	left: 455px;
}

#ribbon .ribbon-link {
  color: #dedede;
  font-family: 'Quattrocento Sans', sans-serif;
  text-align: center; 
  padding-top: 8px;
  z-index: 8;
  position: relative;
}

#ribbon .container {
	position: relative;
	width: 200px;
	height: 75px;
	overflow: hidden;
	margin: 0 auto;
	border-left: 1px solid #37074D;
	border-right: 1px solid #37074D;
}

#ribbon .base {
  width: 200px;
	height: 50px;
	
	background: rgb(55,7,77);
	background: -moz-linear-gradient(top,  rgba(55,7,77,1) 0%, rgba(78,45,96,1) 100%);
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(55,7,77,1)), color-stop(100%,rgba(78,45,96,1)));
	background: -webkit-linear-gradient(top,  rgba(55,7,77,1) 0%,rgba(78,45,96,1) 100%);
	background: -o-linear-gradient(top,  rgba(55,7,77,1) 0%,rgba(78,45,96,1) 100%);
	background: -ms-linear-gradient(top,  rgba(55,7,77,1) 0%,rgba(78,45,96,1) 100%);
	background: linear-gradient(top,  rgba(55,7,77,1) 0%,rgba(78,45,96,1) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#37074D', endColorstr='#4e2d60',GradientType=0 );

	position: relative;
	z-index: 7;
}

#ribbon .base:after {
	content: '';
	position: absolute;
	top: 0;
	width: 186px;
	left: 6px;
	height: 73px;
	border-left: 1px dashed #768c7e;
	border-right: 1px dashed #768c7e;
	z-index: 2;
}

#ribbon .base:before {
	content: '';
	position: absolute;
	top: 0;
	width: 186px;
	left: 7px;
	height: 73px;
	border-left: 1px dashed #768c7e;
	border-right: 1px dashed #768c7e;
	z-index: 2;
}

#ribbon .left_corner {
	width: 150px;
	height: 150px;
	background: #4e2d60;
	position: absolute;
	bottom: 14px;
	left: -20px;
	z-index: 1;
	
	-webkit-transform: rotate(78deg);
	-moz-transform: rotate(78deg);
	-ms-transform: rotate(78deg);
	-o-transform: rotate(78deg);
	transform: rotate(78deg);
}

#ribbon .right_corner {
	width: 150px;
	height: 150px;
	background: #4e2d60;
	position: absolute;
	bottom: 14px;
	right: -20px;
	z-index: 1;
	
	-webkit-transform: rotate(12deg);
	-moz-transform: rotate(12deg);
	-ms-transform: rotate(12deg);
	-o-transform: rotate(12deg);
	transform: rotate(12deg);
}

