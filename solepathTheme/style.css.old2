	-moz-border-radius: 20px;
	-webkit-border-radius: 20px;
	-o-border-radius: 20px;
}
textarea {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 13px;
	color: #444;
	border: 0px;
	padding: 5px;
	margin: 0px;
